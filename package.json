{
  "name": "online-market",
  "version": "1.0.0",
  "engines": {
    "node": "9.6.1"
  },
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "axios": "^0.21.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "express": "^4.16.2",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.3.4",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  },
  "author": "Jawad Zaheer",
  "license": "ISC"
}
